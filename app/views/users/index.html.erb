<% @users.each do |user|%>
<ul> 
  
  <li>
    <%= user.username%>
    <%= form_with url: friends_requests_path, method: :post do |f|%>
        <%= hidden_field_tag 'request[request_sender_id]', current_user.id %>
    <%= f.submit "Add Friend", class: "btn btn-primary" %>
    <% end%>
  </li>

</ul>

<%end%>